<launch>
    <!-- Start package for VIO and connect fly coontroller -->
    <include file="$(find px4_realsense_bridge)/launch/bridge_mavros.launch"/>
    
    <!-- Start fly by Mavlink messages -->
    <include file="$(find lorett_c4s)/launch/c4s.launch"/>
    
    <!-- Start Web control drone -->
    <include file="$(find web_control)/launch/web.launch"/>
    <!-- <include file="$(find sdrr)/launch/sdr.launch"/> -->

    <!-- Start Sateline track mapping -->
    <include file="$(find tracks)/launch/fly_tarck.launch"/>

    <!-- Start Aruco 
    <include file="$(find aruco_pose)/launch/sample.launch"/>
    -->
    <!-- Start Web video Server 
    <node name="web_video_server" pkg="web_video_server" type="web_video_server" output="screen" clear_params="true"/>
-->
</launch> 
